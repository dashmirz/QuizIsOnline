<Project ToolsVersion="4.0" xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
  <PropertyGroup>
    <WebPublishMethod>MSDeploy</WebPublishMethod>
    <ResourceId>/subscriptions/4903a9df-a91c-49b8-bdc7-e158577ec1dd/resourceGroups/OQuizGroup/providers/Microsoft.Web/sites/OnlineQuizz</ResourceId>
    <ResourceGroup>OQuizGroup</ResourceGroup>
    <PublishProvider>AzureWebSite</PublishProvider>
    <LastUsedBuildConfiguration>Release</LastUsedBuildConfiguration>
    <LastUsedPlatform>Any CPU</LastUsedPlatform>
    <SiteUrlToLaunchAfterPublish>https://onlinequizz.azurewebsites.net</SiteUrlToLaunchAfterPublish>
    <LaunchSiteAfterPublish>True</LaunchSiteAfterPublish>
    <ExcludeApp_Data>False</ExcludeApp_Data>
    <MSDeployServiceURL>onlinequizz.scm.azurewebsites.net:443</MSDeployServiceURL>
    <DeployIisAppPath>OnlineQuizz</DeployIisAppPath>
    <RemoteSitePhysicalPath />
    <SkipExtraFilesOnServer>True</SkipExtraFilesOnServer>
    <InstallAspNetCoreSiteExtension>False</InstallAspNetCoreSiteExtension>
    <MSDeployPublishMethod>WMSVC</MSDeployPublishMethod>
    <EnableMSDeployBackup>True</EnableMSDeployBackup>
    <EnableMsDeployAppOffline>False</EnableMsDeployAppOffline>
    <UserName>$OnlineQuizz</UserName>
    <_SavePWD>True</_SavePWD>
    <_DestinationType>AzureWebSite</_DestinationType>
    <PublishDatabaseSettings>
      <Objects xmlns="">
        <ObjectGroup Name="OnlineQuizConn" Order="1" Enabled="False">
          <Destination Path="Data Source=onlinequizz.database.windows.net;Initial Catalog=OnlineQuizz;User ID=onlinequizz;Password=oquiz123!@#" />
          <Object Type="DbDacFx">
            <PreSource Path="Data Source=.;Initial Catalog=OnlineQuizSystem;Integrated Security=True" includeData="False" />
            <Source Path="$(IntermediateOutputPath)AutoScripts\OnlineQuizConn_IncrementalSchemaOnly.dacpac" dacpacAction="Deploy" />
          </Object>
          <UpdateFrom Type="Web.Config">
            <Source MatchValue="Data Source=.;Initial Catalog=OnlineQuizSystem;Integrated Security=True" MatchAttributes="$(UpdateFromConnectionStringAttributes)" />
          </UpdateFrom>
        </ObjectGroup>
      </Objects>
    </PublishDatabaseSettings>
  </PropertyGroup>
  <ItemGroup>
    <MSDeployParameterValue Include="OnlineQuizConn-Web.config Connection String">
      <ParameterValue>Data Source=onlinequizz.database.windows.net;Initial Catalog=OnlineQuizz;User ID=onlinequizz;Password=oquiz123!@#</ParameterValue>
    </MSDeployParameterValue>
  </ItemGroup>
</Project>